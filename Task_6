// 6. Дано вещественное число - цена 1кг конфет. Вывести стоимость 1.2, 1.4, ..., 2кг конфет.
// начал 11:53, закончил 12:20
#include <stdio.h>

int main()
{
	double A=3.42;
	double b;
	
	for(int i=1; i<11; i++)
	{
		if(i%2==0) 
		{	
      b=i*0.1+1;
			printf("%4.2f kg = %4.2f \n", b, b*A);
		}	
		
	}
	
	return 0;
}
