// 2. Даны два целых числа А и В (А<В). Вывести в порядке убывания все целые числа, расположенные между А и В(не включая числа А и В), а так же количество N этих чисел.
// начал 11:10, закончил 11:15
#include <stdio.h>

int main()
{
	int A=3;
	int B=12;
	int N=0;
	for(int i=(B-1); i>(A); i--)
	{
		printf("%d \n", i);
		N++;
	}
	printf("N=%d", N);
	return 0;
}
